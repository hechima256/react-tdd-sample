{
	"$schema": "https://turbo.build/schema.json",
	"globalDependencies": ["**/.env.*local"],
	"pipeline": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": ["dist/**", ".next/**", "!.next/cache/**"]
		},
		"lint": {},
		"dev": {
			"cache": false,
			"persistent": true
		},
		"test": {
			"dependsOn": ["build"],
			"outputs": ["coverage/**"]
		},
		"test:watch": {
			"cache": false,
			"persistent": true
		},
		"test:coverage": {
			"dependsOn": ["build"],
			"outputs": ["coverage/**"]
		},
		"format": {},
		"format:check": {},
		"preview": {
			"dependsOn": ["build"]
		}
	}
}
